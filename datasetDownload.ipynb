   "source": [
    "from sklearn.datasets import load_iris\n",
    "import numpy as np\n",
    "data = load_iris()\n",
    "dataset = np.append(data.data, data.target.reshape([150,1]),axis=1)\n",
    "dataset = np.trunc(dataset).astype(int)\n",
    "np.savetxt(\"iris_int.csv\", dataset, fmt='%d', delimiter=',')"
   ]
  }
